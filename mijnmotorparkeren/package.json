{
  "name": "mijnmotorparkeren",
  "version": "1.0.0",
  "description": "Find motorcycle parking rules per gemeente in the Netherlands",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && node scripts/generate-sitemap.js",
    "preview": "vite preview",
    "test": "vitest",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,md,json}\"",
    "validate:data": "tsx scripts/validate-data.ts",
    "type-check": "tsc --noEmit",
    "sync:gemeentes-index": "tsx scripts/sync-gemeentes-index.ts"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-toast": "^1.2.14",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@radix-ui/react-visually-hidden": "^1.2.3",
    "@tailwindcss/postcss": "^4.1.11",
    "clsx": "^2.1.1",
    "fuse.js": "^7.1.0",
    "idb": "^8.0.3",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.525.0",
    "n": "^10.2.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-helmet-async": "^2.0.5",
    "react-leaflet": "^5.0.0",
    "react-router-dom": "^7.7.0",
    "stable": "^0.1.8",
    "tailwind-merge": "^3.3.1",
    "workbox-build": "^7.3.0",
    "workbox-window": "^7.3.0",
    "zod": "^3.25.76",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/leaflet": "^1.9.19",
    "@types/node": "^24.0.10",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "@vitejs/plugin-react": "^4.6.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.30.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "jsdom": "^26.1.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.11",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "vite": "^7.0.3",
    "vite-plugin-pwa": "^1.0.1",
    "vitest": "^3.2.4"
  }
}
